{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "KeyVault": {
    "VaultUri": "https://kv-policyservice-dev.vault.azure.net/"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=PolicyServiceDb_Dev;Trusted_Connection=true;MultipleActiveResultSets=true;TrustServerCertificate=true",
    "AzureSqlConnectionString": "Will be overridden by Key Vault"
  },
  "EndBookingSystem": {
    "BaseUrl": "https://httpstat.us",
    "TimeoutSeconds": 10
  },
  "Polly": {
    "RetryCount": 3,
    "CircuitBreakerThreshold": 5,
    "CircuitBreakerDurationSeconds": 30,
    "BulkheadMaxParallelization": 10,
    "TimeoutSeconds": 10
  },
  "Hangfire": {
    "DashboardPath": "/hangfire",
    "DashboardTitle": "Policy Service Jobs",
    "WorkerCount": 5,
    "PollingInterval": 15000,
    "DLQAlertThreshold": 10,
    "LogRetentionDays": 30,
    "FileRetentionDays": 90
  },
  "UseAzureSql": false,
  "AllowedHosts": "*"
}